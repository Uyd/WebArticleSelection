@model IEnumerable<LibraryEntity.Files>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*部分view视图*@
@Html.Partial("_FilesTable",Model)
<partial name="_FilesTable" for="@Model" />


@await Component.InvokeAsync("Welcome")
@*自定义taghelper的使用*@
<vc:welcome></vc:welcome>
<script>
    //给表格添加序号
    var item = document.getElementsByClassName('sn');
    for (var i = 0; i < item.length; i++) {
        item[i].innerHTML = i + 1;
    }
</script>
